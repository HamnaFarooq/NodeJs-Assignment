<div class="content">
    <div class="title m-b-md">
        Societies
    </div>
    <div class="row">
        <a href="/societies/create" class="btn btn-primary">Add another Society</a>
    </div>

    <div class="row">
        <% for(var i=0; i < societies.length; i++) { const society = societies[i] %>

        <div class="col col-md-3">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title"><%- society.socname %></h5>
                    <p class="card-text"><%- society.type %> </p>
                    <a href="/societies/<%- society.socid %>/edit" class="btn btn-primary">Edit</a>
                    <a href="/societies/<%- society.socid %>/delete" class="btn btn-danger">Delete</a>
                </div>
            </div>
        </div>

        <% } %>

    </div>



</div>

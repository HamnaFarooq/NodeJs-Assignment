<a href="/societies" class="btn"> Go Back </a>

    <div class="container-fluid">
      <div class="container">
        <h2>Edit society</h2>
        <form action="/societies/<%- society.socid %>" method="POST">

          <div class="form-group">
            <label for="socname">Society Name:</label>
            <input type="text" class="form-control" name="socname" value="<%- society.socname %>" placeholder="Enter Society name">
          </div>

          <div class="form-group">
            <label for="type">Society type:</label>
            <input type="text" class="form-control" name="type" value="<%- society.type %>" placeholder="Enter Society type" >
          </div>

          <button type="submit" class="btn btn-primary">Update</button>

        </form>
        </div>
    	</div>

<div class="content">
    <div class="title m-b-md">
        sessions
    </div>

    <div class="row">
        <a href="/sessions/create" class="btn btn-primary">Add another session</a>
    </div>

    <div class="row">
        <% for(var i=0; i < sessions.length; i++) { const session = sessions[i] %>

        <div class="col col-md-3">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title"><%-session.stitle%></h5>
                    <p class="card-text"><%-session.sdate%></p>
                    <a href="/sessions/<%- session.sesid %>/edit" class="btn btn-primary">Edit</a>
                    <a href="/sessions/<%- session.sesid %>/delete" class="btn btn-danger">Delete</a>
                </div>
            </div>
        </div>

        <%}%>

    </div>
</div>